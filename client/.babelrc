{
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es"
      }
    ],
    [
      "import",
      {
        "libraryName": "@ant-design/icons",
        "libraryDirectory": "es/icons",
        "camel2DashComponentName": false
      },
      "import-antd-icons"
    ]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}